{
	"name": "logzen",
	"version": "0.3.8",
	"description": "An easy to use utility for logging",
	"main": "dist/logzen.min.js",
	"types": "dist",
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*"
			]
		}
	},
	"scripts": {
		"format:check": "prettier --check .",
		"format": "prettier --write .",
		"lint": "eslint src && tsc --noEmit",
		"build": "tsx scripts/build.ts",
		"build:docs": "typedoc src/index.ts --out docs",
		"test": "jest",
		"prepublishOnly": "npm run format && npm run lint && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dr-vortex/logzen.git"
	},
	"keywords": [
		"log",
		"logger"
	],
	"author": "Dr. Vortex",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dr-vortex/logzen/issues"
	},
	"homepage": "https://github.com/dr-vortex/logzen#readme",
	"devDependencies": {
		"@types/jest": "^29.5.3",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^7.7.1",
		"@typescript-eslint/parser": "^7.7.1",
		"esbuild": "^0.20.2",
		"eslint": "^8.45.0",
		"jest": "^29.7.0",
		"prettier": "^3.0.0",
		"ts-jest": "^29.1.2",
		"tsx": "^4.7.2",
		"typedoc": "^0.25.13",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"eventemitter3": "^5.0.1"
	}
}
